<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finance.dao.StorageLogisticsMapper">
	<select id="findById" parameterType="int" resultType="StorageLogistics">
		SELECT *
		FROM storage_logistics WHERE id=#{id,jdbcType=INTEGER}
	</select>
	<insert id="insert" parameterType="StorageLogistics" flushCache="true"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO
		storage_logistics(companyName,registrationNumber,address,tel,email,logisticsEmpSum,logisticianEmpNum,isGradeA,isRentStorage,storageArea,storageCapacity,storageType,stockType,totalThroughput,storageAreaAverageUtilization,storageAreaUtilization,serviceObject,totalValueOfOutput,totalVariableCost,totaFixedCost,year,lng,lat)
		VALUES(#{companyName,jdbcType=VARCHAR},#{registrationNumber,jdbcType=VARCHAR},#{address,jdbcType=VARCHAR},#{tel,jdbcType=VARCHAR},#{email,jdbcType=VARCHAR},#{logisticsEmpSum,jdbcType=SMALLINT},#{logisticianEmpNum,jdbcType=SMALLINT},#{isGradeA,jdbcType=TINYINT},#{isRentStorage,jdbcType=VARCHAR},#{storageArea,jdbcType=FLOAT},#{storageCapacity,jdbcType=FLOAT},#{storageType,jdbcType=VARCHAR},#{stockType,jdbcType=VARCHAR},#{totalThroughput,jdbcType=FLOAT},#{storageAreaAverageUtilization,jdbcType=FLOAT},#{storageAreaUtilization,jdbcType=FLOAT},#{serviceObject,jdbcType=VARCHAR},#{totalValueOfOutput,jdbcType=FLOAT},#{totalVariableCost,jdbcType=FLOAT},#{totaFixedCost,jdbcType=FLOAT},#{year,jdbcType=VARCHAR},#{lng},#{lat})
	</insert>
	<update id="update" parameterType="StorageLogistics">
		UPDATE storage_logistics SET
		lng=#{lng},lat=#{lat},companyName=#{companyName,jdbcType=VARCHAR},registrationNumber=#{registrationNumber,jdbcType=VARCHAR},address=#{address,jdbcType=VARCHAR},tel=#{tel,jdbcType=VARCHAR},email=#{email,jdbcType=VARCHAR},logisticsEmpSum=#{logisticsEmpSum,jdbcType=SMALLINT},logisticianEmpNum=#{logisticianEmpNum,jdbcType=SMALLINT},isGradeA=#{isGradeA,jdbcType=TINYINT},isRentStorage=#{isRentStorage,jdbcType=VARCHAR},storageArea=#{storageArea,jdbcType=FLOAT},storageCapacity=#{storageCapacity,jdbcType=FLOAT},storageType=#{storageType,jdbcType=VARCHAR},stockType=#{stockType,jdbcType=VARCHAR},totalThroughput=#{totalThroughput,jdbcType=FLOAT},storageAreaAverageUtilization=#{storageAreaAverageUtilization,jdbcType=FLOAT},storageAreaUtilization=#{storageAreaUtilization,jdbcType=FLOAT},serviceObject=#{serviceObject,jdbcType=VARCHAR},totalValueOfOutput=#{totalValueOfOutput,jdbcType=FLOAT},totalVariableCost=#{totalVariableCost,jdbcType=FLOAT},totaFixedCost=#{totaFixedCost,jdbcType=FLOAT},year=#{year,jdbcType=VARCHAR}
		WHERE id=#{id,jdbcType=INTEGER}
	</update>
	<delete id="delete" parameterType="int">
		DELETE FROM storage_logistics
		WHERE id=#{id,jdbcType=INTEGER}
	</delete>
	<select id="findAll" resultType="StorageLogistics">
		SELECT * FROM storage_logistics where address is not null and lng is not null and lat is not null
	</select>
	<select id="searchByPage" resultType="StorageLogistics">
		SELECT * FROM storage_logistics
		<where>
			<if test="l.companyName != null and l.companyName!=''">
				`companyName` like
				CONCAT('%',#{l.companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="l.registrationNumber != null and l.registrationNumber!=''">
				and `registrationNumber` like
				CONCAT('%',#{l.registrationNumber,jdbcType=VARCHAR},'%')
			</if>
		</where>
		limit #{pageUtil.begin},#{pageUtil.length}
	</select>
	<select id="findAllMap" resultType="StorageLogistics">
		SELECT * FROM storage_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="address != null and address!=''">
				and `address` like
				CONCAT('%',#{address,jdbcType=VARCHAR},'%')
			</if>
			<if test="tel != null and tel!=''">
				and `tel` like
				CONCAT('%',#{tel,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCount" resultType="long">
		SELECT COUNT(*) FROM `storage_logistics`
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="registrationNumber != null and registrationNumber!=''">
				and `registrationNumber` like
				CONCAT('%',#{registrationNumber,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<insert id="batchInsert" parameterType="java.util.List">  
	    insert into storage_logistics (companyName,registrationNumber,address,tel,email,logisticsEmpSum,logisticianEmpNum,isGradeA,isRentStorage,storageArea,storageCapacity,storageType,stockType,totalThroughput,storageAreaAverageUtilization,storageAreaUtilization,serviceObject,totalValueOfOutput,totalVariableCost,totaFixedCost,year) values  
	    <foreach collection="list" item="item" index="index"  
	        separator=",">  
	        ( #{item.item0}, #{item.item1}, #{item.item2}, #{item.item3}, #{item.item4}, #{item.item5}, #{item.item6}, #{item.item7}, #{item.item8}, #{item.item9}, #{item.item10}, #{item.item11}, #{item.item12}, #{item.item13}, #{item.item14}, #{item.item15}, #{item.item16}, #{item.item17}, #{item.item18}, #{item.item19}, #{item.item20})  
	    </foreach>
  </insert> 
  
  <select id="searchDownload" resultType="java.util.Map">
		SELECT * FROM storage_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="registrationNumber != null and registrationNumber!=''">
				and `registrationNumber` like
				CONCAT('%',#{registrationNumber,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCountNOLocation" resultType="long">
		SELECT COUNT(*) FROM `storage_logistics` where (lng is null or lat is null) and address is not null
	</select>
	
	<select id="getOne" resultType="java.util.Map">
		SELECT * FROM `storage_logistics` where (lng is null or lat is null) and address is not null limit 1
	</select>
	
	<update id="updateNOLocation" parameterType="java.util.Map">
		update storage_logistics set lng = #{lng} , lat = #{lat} where id = #{id}
	</update>
</mapper>
