<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finance.dao.MultipleshopLogisticsMapper">
	<select id="findById" parameterType="int" resultType="MultipleshopLogistics">
		SELECT *
		FROM multipleshop_logistics WHERE id=#{id,jdbcType=INTEGER}
	</select>
	<insert id="insert" parameterType="MultipleshopLogistics" flushCache="true"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO
		multipleshop_logistics(companyName,address,linkman,tel,email,headOfficeType,headOfficeAddress,logisticsType,logisticsEmpSum,logisticianEmpNum,storageArea,storageTemperature,carType,carNum,businessAdvantage,businessDisadvantage,totalValueOfOutput,profitsTax,valueOfOutput,profitsTaxFive,orderQuantity,year,lng,lat)
		VALUES(#{companyName,jdbcType=VARCHAR},#{address,jdbcType=VARCHAR},#{linkman,jdbcType=VARCHAR},#{tel,jdbcType=VARCHAR},#{email,jdbcType=VARCHAR},#{headOfficeType,jdbcType=VARCHAR},#{headOfficeAddress,jdbcType=VARCHAR},#{logisticsType,jdbcType=VARCHAR},#{logisticsEmpSum,jdbcType=SMALLINT},#{logisticianEmpNum,jdbcType=SMALLINT},#{storageArea,jdbcType=FLOAT},#{storageTemperature,jdbcType=FLOAT},#{carType,jdbcType=VARCHAR},#{carNum,jdbcType=SMALLINT},#{businessAdvantage,jdbcType=VARCHAR},#{businessDisadvantage,jdbcType=VARCHAR},#{totalValueOfOutput,jdbcType=FLOAT},#{profitsTax,jdbcType=FLOAT},#{valueOfOutput,jdbcType=FLOAT},#{profitsTaxFive,jdbcType=FLOAT},#{orderQuantity,jdbcType=SMALLINT},#{year,jdbcType=VARCHAR},#{lng},#{lat})
	</insert>
	<update id="update" parameterType="MultipleshopLogistics">
		UPDATE multipleshop_logistics SET
		lng=#{lng},lat=#{lat},companyName=#{companyName,jdbcType=VARCHAR},address=#{address,jdbcType=VARCHAR},linkman=#{linkman,jdbcType=VARCHAR},tel=#{tel,jdbcType=VARCHAR},email=#{email,jdbcType=VARCHAR},headOfficeType=#{headOfficeType,jdbcType=VARCHAR},headOfficeAddress=#{headOfficeAddress,jdbcType=VARCHAR},logisticsType=#{logisticsType,jdbcType=VARCHAR},logisticsEmpSum=#{logisticsEmpSum,jdbcType=SMALLINT},logisticianEmpNum=#{logisticianEmpNum,jdbcType=SMALLINT},storageArea=#{storageArea,jdbcType=FLOAT},storageTemperature=#{storageTemperature,jdbcType=FLOAT},carType=#{carType,jdbcType=VARCHAR},carNum=#{carNum,jdbcType=SMALLINT},businessAdvantage=#{businessAdvantage,jdbcType=VARCHAR},businessDisadvantage=#{businessDisadvantage,jdbcType=VARCHAR},totalValueOfOutput=#{totalValueOfOutput,jdbcType=FLOAT},profitsTax=#{profitsTax,jdbcType=FLOAT},valueOfOutput=#{valueOfOutput,jdbcType=FLOAT},profitsTaxFive=#{profitsTaxFive,jdbcType=FLOAT},orderQuantity=#{orderQuantity,jdbcType=SMALLINT},year=#{year,jdbcType=VARCHAR}
		WHERE id=#{id,jdbcType=INTEGER}
	</update>
	<delete id="delete" parameterType="int">
		DELETE FROM multipleshop_logistics
		WHERE id=#{id,jdbcType=INTEGER}
	</delete>
	<select id="findAll" resultType="MultipleshopLogistics">
		SELECT * FROM multipleshop_logistics where address is not null and lng is not null and lat is not null
	</select>
	<select id="searchByPage" resultType="MultipleshopLogistics">
		SELECT * FROM multipleshop_logistics
		<where>
			<if test="l.companyName != null and l.companyName!=''">
				`companyName` like
				CONCAT('%',#{l.companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="l.linkman != null and l.linkman!=''">
				and `linkman` like
				CONCAT('%',#{l.linkman,jdbcType=VARCHAR},'%')
			</if>
		</where>
		limit #{pageUtil.begin},#{pageUtil.length}
	</select>
	<select id="findAllMap" resultType="MultipleshopLogistics">
		SELECT * FROM multipleshop_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="address != null and address!=''">
				and `address` like
				CONCAT('%',#{address,jdbcType=VARCHAR},'%')
			</if>
			<if test="tel != null and tel!=''">
				and `tel` like
				CONCAT('%',#{tel,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCount" resultType="long">
		SELECT COUNT(*) FROM `multipleshop_logistics`
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="linkman != null and linkman!=''">
				and `linkman` like
				CONCAT('%',#{linkman,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<insert id="batchInsert" parameterType="java.util.List">  
	    insert into multipleshop_logistics (companyName,address,linkman,tel,email,headOfficeType,headOfficeAddress,logisticsType,logisticsEmpSum,logisticianEmpNum,storageArea,storageTemperature,carType,carNum,businessAdvantage,businessDisadvantage,totalValueOfOutput,profitsTax,valueOfOutput,profitsTaxFive,orderQuantity,year) values  
	    <foreach collection="list" item="item" index="index"  
	        separator=",">  
	        ( #{item.item0}, #{item.item1}, #{item.item2}, #{item.item3}, #{item.item4}, #{item.item5}, #{item.item6}, #{item.item7}, #{item.item8}, #{item.item9}, #{item.item10}, #{item.item11}, #{item.item12}, #{item.item13}, #{item.item14}, #{item.item15}, #{item.item16}, #{item.item17}, #{item.item18}, #{item.item19}, #{item.item20}, #{item.item21})  
	    </foreach>
  </insert> 
  
  <select id="searchDownload" resultType="java.util.Map">
		SELECT * FROM multipleshop_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="linkman != null and linkman!=''">
				and `linkman` like
				CONCAT('%',#{linkman,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCountNOLocation" resultType="long">
		SELECT COUNT(*) FROM `multipleshop_logistics` where (lng is null or lat is null) and address is not null
	</select>
	
	<select id="getOne" resultType="java.util.Map">
		SELECT * FROM `multipleshop_logistics` where (lng is null or lat is null) and address is not null limit 1
	</select>
	
	<update id="updateNOLocation" parameterType="java.util.Map">
		update multipleshop_logistics set lng = #{lng} , lat = #{lat} where id = #{id}
	</update>
</mapper>
