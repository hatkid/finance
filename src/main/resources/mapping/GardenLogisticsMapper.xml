<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finance.dao.GardenLogisticsMapper">
	<select id="findById" parameterType="int" resultType="GardenLogistics">
		SELECT *
		FROM garden_logistics WHERE id=#{id,jdbcType=INTEGER}
	</select>
	<insert id="insert" parameterType="GardenLogistics" flushCache="true"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO
		garden_logistics(companyName,address,legalPerson,tel,email,achieveDistinction,gardenType,cmpNumInGarden,storageArea,vehiclesNumber,occupancyRate,haveInformationPlatform,haveLandDeed,areaOfLand,businessAdvantage,businessDisadvantage,totalValueOfOutput,profitsTax,valueOfOutput,profitsTaxFive,year,lng,lat)
		VALUES(#{companyName,jdbcType=VARCHAR},#{address,jdbcType=VARCHAR},#{legalPerson,jdbcType=VARCHAR},#{tel,jdbcType=VARCHAR},#{email,jdbcType=VARCHAR},#{achieveDistinction,jdbcType=VARCHAR},#{gardenType,jdbcType=VARCHAR},#{cmpNumInGarden,jdbcType=SMALLINT},#{storageArea,jdbcType=FLOAT},#{vehiclesNumber,jdbcType=SMALLINT},#{occupancyRate,jdbcType=FLOAT},#{haveInformationPlatform,jdbcType=TINYINT},#{haveLandDeed,jdbcType=TINYINT},#{areaOfLand,jdbcType=FLOAT},#{businessAdvantage,jdbcType=VARCHAR},#{businessDisadvantage,jdbcType=VARCHAR},#{totalValueOfOutput,jdbcType=FLOAT},#{profitsTax,jdbcType=FLOAT},#{valueOfOutput,jdbcType=FLOAT},#{profitsTaxFive,jdbcType=FLOAT},#{year,jdbcType=VARCHAR},#{lng},#{lat})
	</insert>
	<update id="update" parameterType="GardenLogistics">
		UPDATE garden_logistics SET
		lng=#{lng},lat=#{lat},companyName=#{companyName,jdbcType=VARCHAR},address=#{address,jdbcType=VARCHAR},legalPerson=#{legalPerson,jdbcType=VARCHAR},tel=#{tel,jdbcType=VARCHAR},email=#{email,jdbcType=VARCHAR},achieveDistinction=#{achieveDistinction,jdbcType=VARCHAR},gardenType=#{gardenType,jdbcType=VARCHAR},cmpNumInGarden=#{cmpNumInGarden,jdbcType=SMALLINT},storageArea=#{storageArea,jdbcType=FLOAT},vehiclesNumber=#{vehiclesNumber,jdbcType=SMALLINT},occupancyRate=#{occupancyRate,jdbcType=FLOAT},haveInformationPlatform=#{haveInformationPlatform,jdbcType=TINYINT},haveLandDeed=#{haveLandDeed,jdbcType=TINYINT},areaOfLand=#{areaOfLand,jdbcType=FLOAT},businessAdvantage=#{businessAdvantage,jdbcType=VARCHAR},businessDisadvantage=#{businessDisadvantage,jdbcType=VARCHAR},totalValueOfOutput=#{totalValueOfOutput,jdbcType=FLOAT},profitsTax=#{profitsTax,jdbcType=FLOAT},valueOfOutput=#{valueOfOutput,jdbcType=FLOAT},profitsTaxFive=#{profitsTaxFive,jdbcType=FLOAT},year=#{year,jdbcType=VARCHAR}
		WHERE id=#{id,jdbcType=INTEGER}
	</update>
	<delete id="delete" parameterType="int">
		DELETE FROM garden_logistics
		WHERE id=#{id,jdbcType=INTEGER}
	</delete>
	<select id="findAll" resultType="GardenLogistics">
		SELECT * FROM garden_logistics where address is not null and lng is not null and lat is not null
	</select>
	<select id="searchByPage" resultType="GardenLogistics">
		SELECT * FROM garden_logistics
		<where>
			<if test="l.companyName != null and l.companyName!=''">
				`companyName` like
				CONCAT('%',#{l.companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="l.legalPerson != null and l.legalPerson!=''">
				and `legalPerson` like
				CONCAT('%',#{l.legalPerson,jdbcType=VARCHAR},'%')
			</if>
		</where>
		limit #{pageUtil.begin},#{pageUtil.length}
	</select>
	<select id="findAllMap" resultType="GardenLogistics">
		SELECT * FROM garden_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="address != null and address!=''">
				and `address` like
				CONCAT('%',#{address,jdbcType=VARCHAR},'%')
			</if>
			<if test="tel != null and tel!=''">
				and `tel` like
				CONCAT('%',#{tel,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCount" resultType="long">
		SELECT COUNT(*) FROM `garden_logistics`
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="legalPerson != null and legalPerson!=''">
				and `legalPerson` like
				CONCAT('%',#{legalPerson,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<insert id="batchInsert" parameterType="java.util.List">  
	    insert into garden_logistics (companyName,address,legalPerson,tel,email,achieveDistinction,gardenType,cmpNumInGarden,storageArea,vehiclesNumber,occupancyRate,haveInformationPlatform,haveLandDeed,areaOfLand,businessAdvantage,businessDisadvantage,totalValueOfOutput,profitsTax,valueOfOutput,profitsTaxFive,year) values  
	    <foreach collection="list" item="item" index="index"  
	        separator=",">  
	        ( #{item.item0}, #{item.item1}, #{item.item2}, #{item.item3}, #{item.item4}, #{item.item5}, #{item.item6}, #{item.item7}, #{item.item8}, #{item.item9}, #{item.item10}, #{item.item11}, #{item.item12}, #{item.item13}, #{item.item14}, #{item.item15}, #{item.item16}, #{item.item17}, #{item.item18}, #{item.item19}, #{item.item20})  
	    </foreach>
  </insert> 
  
  <select id="searchDownload" resultType="java.util.Map">
		SELECT * FROM garden_logistics
		<where>
			<if test="companyName != null and companyName!=''">
				`companyName` like
				CONCAT('%',#{companyName,jdbcType=VARCHAR},'%')
			</if>
			<if test="legalPerson != null and legalPerson!=''">
				and `legalPerson` like
				CONCAT('%',#{legalPerson,jdbcType=VARCHAR},'%')
			</if>
		</where>
	</select>
	<select id="getCountNOLocation" resultType="long">
		SELECT COUNT(*) FROM `garden_logistics` where (lng is null or lat is null) and address is not null
	</select>
	
	<select id="getOne" resultType="java.util.Map">
		SELECT * FROM `garden_logistics` where (lng is null or lat is null) and address is not null limit 1
	</select>
	
	<update id="updateNOLocation" parameterType="java.util.Map">
		update garden_logistics set lng = #{lng} , lat = #{lat} where id = #{id}
	</update>
</mapper>
